{
  "name": "hubot-stride",
  "version": "1.0.3",
  "description": "Hubot adapter for Atlassian Stride",
  "main": "lib/web.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "eval $(cat .env.dev) hubot -a stride",
    "test": "coffee -c node_modules/hubot && NODE_ENV=testing jest -i",
    "test:coverage": "coffee -c node_modules/hubot && NODE_ENV=testing jest -i --coverage",
    "test:watch": "coffee -c node_modules/hubot && NODE_ENV=testing jest -i --watch",
    "test:ci": "coffee -c node_modules/hubot && NODE_ENV=testing jest --coverage --testResultsProcessor='jest-junit' -i --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/atlassian/hubot-stride.git"
  },
  "keywords": [
    "hubot",
    "stride",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://bitbucket.org/atlassian/hubot-stride#readme",
  "peerDependencies": {
    "hubot": ">=2.0"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "coffee-script": "^1.12.7",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "hubot": "^2.19.0",
    "jest": "^22.4.2",
    "jest-junit": "^3.6.0",
    "nock": "^9.2.3",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "adf-builder": "^3.1.0",
    "bluebird": "^3.5.1",
    "compression": "^1.7.2",
    "convict": "^4.0.2",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.5",
    "node-fetch": "^2.0.0",
    "require-directory": "^2.1.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/helpers/",
      "/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "clover"
    ],
    "coverageDirectory": "./test-results",
    "collectCoverage": true
  },
  "jest-junit": {
    "output": "./test-results/test-results.xml"
  }
}
